{
    "name": "brilliantpackages/laravel-fmerrorhelper",
    "description": "A package to convert FileMaker Pro error codes to HTTP status codes and messages.",
    "license": "MIT",
    "keywords": ["error", "status", "rest", "rest-api", "api", "filemaker", "filemaker pro", "laravel", "lumen"],
    "homepage": "https://github.com/LuminFire/Laravel-FileMaker-Error-Helper",
    "type": "library",
    "authors": [
        {
            "name": "LuminFire",
            "email": "help@luminfire.com",
            "role": "Owner"
        },
        {
            "name": "Andrew Minion",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "orchestra/testbench": "~3.8.0|^4.0|^5.0",
        "nunomaduro/collision": "^3||^4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "BrilliantPackages\\LaravelFmErrorHelper\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrilliantPackages\\LaravelFmErrorHelper\\LaravelFmErrorHelperServiceProvider"
            ],
            "aliases": {
                "FMError": "BrilliantPackages\\LaravelFmErrorHelper\\FMError"
            }
        }
    }
}
